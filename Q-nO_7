def intersection(nums1, nums2):
    set1 = set(nums1)
    intersection = []

    for num in nums2:
        if num in set1 and num not in intersection:
            intersection.append(num)

    return intersection
nums1 = [1, 2, 2, 1]
nums2 = [2, 2]
result1 = intersection(nums1, nums2)
print(result1)

nums3 = [4, 9, 5]
nums4 = [9, 4, 9, 8, 4]
result2 = intersection(nums3, nums4)
print(result2)
[2]
[9, 4]
